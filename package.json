{
  "name": "Star-Wars",
  "version": "1.0.0",
  "main": "index.js",
  "scripts": {
    "build:css": "postcss src/styles/tailwind.css -o src/styles.css",
    "dev": "yarn build:css && next ./src",
    "build": "yarn build:css && next build ./src",
    "start": "yarn build:css && next start ./src",
    "test": "jest"
  },
  "license": "ISC",
  "dependencies": {
    "axios": "^0.21.1",
    "file-loader": "^6.2.0",
    "gsap": "^3.6.1",
    "next": "^10.1.2",
    "next-react-svg": "^1.1.2",
    "postcss-import": "^14.0.0",
    "postcss-loader": "^5.2.0",
    "react": "^17.0.2",
    "react-dom": "^17.0.2",
    "url-loader": "^4.1.1"
  },
  "repository": "https://github.com/nelbe/StarWars-SPA.git",
  "author": "Belen Dominguez",
  "devDependencies": {
    "@babel/preset-env": "^7.13.12",
    "@babel/preset-react": "^7.13.13",
    "@fullhuman/postcss-purgecss": "^4.0.3",
    "autoprefixer": "^10.2.5",
    "babel-jest": "^26.6.3",
    "jest": "^26.6.3",
    "postcss": "*",
    "postcss-cli": "^8.3.1",
    "postcss-preset-env": "^6.7.0",
    "react-test-renderer": "^17.0.2",
    "tailwindcss": "^2.0.4"
  },
  "jest": {
    "verbose": true,
    "clearMocks": true,
    "collectCoverage": true,
    "transform": {
      "^.+\\.(ts|tsx|js|jsx)$": "babel-jest"
    },
    "transformIgnorePatterns": [
      "<rootDir>/node_modules/"
    ],
    "moduleNameMapper": {
      "\\.(jpg|jpeg|png|gif|eot|otf|webp|svg|ttf|woff|woff2|mp4|webm|wav|mp3|m4a|aac|oga)$": "<rootDir>/src/pages",
      "\\.(css|scss)$": "identity-obj-proxy"
    }
  }
}
